<script type="text/javascript" charset="utf-8">
  (function(d) {
     window.jstagAsyncInit = function() {
       jstag.init({ cid:"1423" ,loadid: true});
       // now send data capture
       jstag.send();
     }
     var js, id = 'jstag-csdk', ref = d.getElementsByTagName('script')[0];
     if (d.getElementById(id)) return;
     js = d.createElement('script');
     js.id = id;
     js.async = true;
     js.src = "//c.lytics.io/static/io.min.js";
     ref.parentNode.insertBefore(js, ref);
  }(document));
</script>

<script> 
//for current mailchimp sign up form
$("input[id='mc-embedded-subscribe']").click(function(){
   var mc_email = $("input[name='EMAIL']").val();
   console.log(mc_email);
   jstag.send({ email:mc_email });
 })

</script>