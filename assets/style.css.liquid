/* A few high-level styles, mostly having to do with fonts. */ 

body {
  font-family: 'WWLstandard', sans-serif;
  font-weight: normal;
}

h1 {
  font-family: 'WWLheavy', sans-serif;
  font-size: 26px;
}

h2 {
  border-top: 1px solid #777;
  border-bottom: 1px solid #777;
  padding-left: 0px;
  padding-top: 10px;
  padding-bottom: 10px;
  font-family: 'WWLheavy';
  font-size: 18px;
  color: #777;
  width: 100%;
}

a {
  color: #333;
  text-decoration: underline;
}

a:hover {
  color: white;
  background-color: black;
}

area:hover {
  cursor: pointer;
}

b, strong {
  font-family: 'WWLheavy', sans-serif;
  font-weight: normal;
}

.hidden {
  display: none;
  visibility: hidden;
}


/* Containers for the Bootstrap grid. Includes standard responsive container, and a fluid container that expands to the full width of large desktop screens. */

.container {
  margin-bottom: 60px;
}

.fluid-container {
  padding-left: 1px;
  padding-right: 15px;
  margin-left: auto;
  margin-right: auto;
  margin-bottom: 60px;
  margin-top: 40px;
}


/* Styles for two-column pages. */

.left-col {
  padding-top: 50px;
}

.left-col img {
  max-width: 100%;
} 

.right-col {
  padding-top: 60px;
  padding-bottom: 40px;
  text-align: center;
}

@media only screen and (max-width: 991px) {
  .right-col {
    padding-top: 10px;
    padding-bottom: 10px;
  }
}


/* Styles for the top navigation menu. */

.navbar {
  border: none;
}

#header-ribbon {
  width: 100%;
  background-color: black;
  text-align: center;
  padding-top: 5px;
}

#header-ribbon a,
#header-ribbon a:hover {
  font-family: 'WWLlogo';
  font-size: 26px;
  color: white;
  text-decoration: none;
  background-color: black;
}

.navbar-brand {
  font-family: 'WWLlogo';
  text-decoration: none;
  color: white;
  background-color: #333;
}

.navbar-header {
  background-color: #333;
}

#top-menu {
  background-color: #222;
  margin: 0 auto;
  position: relative;
  z-index: 3;
}

#top-menu li a {
  font-family: 'WWLheavy', sans-serif; 
  font-size: 12px;
  color: #BBB;
  text-decoration: none;
  padding-top: 16px;
  padding-bottom: 15px;
} 

#top-menu li a:hover {
  background-color: #DDD;
  color: black;
}

.cart-link {
  margin-left: 100px;
  margin-right: -170px;
/*  
  margin-left: 0px;
  margin-right: -160px; 
*/
}

#sticky-mini-menu {
  margin: 0 auto;
  position: fixed;
  top: 0px;
  left: 0px;
  z-index: 3;
  background-color: #222;
  width: 100%;
  height: 0px;
}

#sticky-mini-menu nav {
  background-color: #222;
}

#sticky-mini-menu li a {
  font-family: 'WWLheavy', sans-serif; 
  font-size: 12px;
  color: #BBB;
  text-decoration: none;
  padding-top: 16px;
  padding-bottom: 15px;
} 

#sticky-mini-menu li a:hover {
  color: black;
  background-color: #DDD;
}

@media only screen and (min-width: 768px) {
  .navbar-nav {
    float: none;
    margin: 0 auto;
    display: table;
    table-layout: fixed;
  }
}

@media only screen and (min-width: 768px) and (max-width: 991px) {
  #nav-the-concept, #nav-about {
    display: none;
  }
}

#nav-submenu-about,
